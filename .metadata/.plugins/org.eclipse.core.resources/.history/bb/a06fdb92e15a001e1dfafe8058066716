package view;
import java.awt.Color;
import java.awt.Component;

import javax.swing.*;
public class PHManagement extends JSplitPane {
	private static JPanel profilePane =  new JPanel();
	private static JPanel holidayPane = new JPanel();
	public PHManagement() {
		super(JSplitPane.HORIZONTAL_SPLIT,profilePane,holidayPane);
		setDividerLocation(500);
		profilePane.setBackground(Color.blue);
		
	}
}
